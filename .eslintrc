{
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "requireConfigFile": false,
    "babelOptions": {
      "presets": ["@babel/preset-env", "@babel/preset-react"]
    }
  },
  "globals": {
    "$": "readonly",
    "jQuery": "readonly"
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "env": {
    "browser": true,
    "node": true,
    "jquery": true
  },
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "rules": {
    "react/prop-types": 0,
    "no-unused-vars": 1,
    "react/react-in-jsx-scope": "off",
    "no-redeclare": "off",
    "no-undef": "off"
  }
}
